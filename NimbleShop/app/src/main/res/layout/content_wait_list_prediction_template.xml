<ScrollView android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/scrollView"
    android:layout_column="0"
    android:paddingTop="50dp"
    xmlns:android="http://schemas.android.com/apk/res/android">


    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/tableTemplate"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:shrinkColumns="*"
        android:stretchColumns="0,2,3"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:context="com.sjsu.nimbleshop.WaitListPredictionTemplateActivity">


     <TableRow
        android:id="@+id/headerRow"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >


        <TextView
            android:id="@+id/storeName"
            android:layout_width="100sp"
            android:layout_column="1"
            android:layout_weight="1"
            android:gravity="left"
            android:text="Store"
            android:textSize="17sp"
            android:textStyle="bold"
            />
        <!--android:layout_column="1" android:padding="20dip"-->
        <TextView
            android:id="@+id/waitTime"
            android:layout_width="50sp"
            android:layout_column="2"
            android:layout_weight="1"
            android:gravity="left"
            android:paddingRight="2dip"
            android:text="Wait time"
            android:textSize="17sp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/totalTime"
            android:layout_width="60sp"
            android:layout_column="3"
            android:layout_weight="1"
            android:gravity="left"
            android:paddingRight="5dip"
            android:text="Total time"
            android:textSize="17sp"
            android:textStyle="bold" />

        <!---->

    </TableRow>

        <!--<TableRow-->
            <!--android:id="@+id/emptyRow"-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="match_parent">-->
