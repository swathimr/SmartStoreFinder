<?xml version="1.0" encoding="utf-8"?>
   <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:shrinkColumns="*"
    android:stretchColumns="0,2,3"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:id="@+id/tableTemplate"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.sjsu.nimbleshop.WaitListPredictionTemplateActivity">


    <TableRow
        android:id="@+id/headerRow"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        >
        <TextView
            android:text="Store Name"
            android:layout_weight="1"
            android:textStyle="normal"
            android:textSize="18sp"
            android:layout_width="100dip"
            android:gravity="center"
            android:id="@+id/storeName"
            />
        <!--android:layout_column="1" android:padding="20dip"-->
        <TextView
            android:text="Wait time"
            android:layout_weight="1"
            android:paddingRight="2dip"
            android:textStyle="normal"
            android:textSize="18sp"
            android:layout_width="50dip"
            android:gravity="center"
            android:id="@+id/waitTime"
            />
        <TextView
            android:text="Total time"
            android:layout_weight="1"
            android:paddingRight="5dip"
            android:textStyle="normal"
            android:textSize="18sp"
            android:layout_width="60dip"
            android:gravity="center"
            android:id="@+id/totalTime"
            />
        <TextView
            android:text="Cost"
            android:layout_weight="1"
            android:paddingRight="5dip"
            android:layout_width="60dip"
            android:textStyle="normal"
            android:textSize="18sp"
            android:gravity="center"
            android:id="@+id/cost"
            />
        <TextView
            android:text="Qty"
            android:layout_weight="1"
            android:paddingRight="5dip"
            android:textStyle="normal"
            android:textSize="18sp"
            android:gravity="center"
            android:id="@+id/qty"
            />
        <!--<Button
            android:id="@+id/navigation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:minWidth="150dp"
            android:layout_marginTop="410dp"
            android:layout_below="@+id/autoCompleteTextView"
            android:text="Get Locations"
            android:background = "@drawable/roundedbutton"
            android:onClick="mapview" />-->

    </TableRow>




</TableLayout>